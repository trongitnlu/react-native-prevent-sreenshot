{
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "react-native-prevent-screenshot": ["./src/index"]
    },
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "lib": ["es2015", "es2016", "esnext"],
    "jsx": "react-native",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
